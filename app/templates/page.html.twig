<header role="banner">
  {% if site_name or site_slogan %}
    <div class="name-and-slogan page-width">

    {# Use h1 when the content title is empty #}
    {% if title | title != '' %}
      <strong class="site-name">
        <a href="{{ front_page }}" title="{{ 'Home' }}" rel="home">{{ site_name }}</a>
      </strong>
    {% else %}
      <h1 class="site-name">
        <a href="{{ front_page }}" title="{{ 'Home' }}" rel="home">{{ site_name }}</a>
      </h1>
    {% endif %}

    {% if site_slogan %}
      <div class="site-slogan content page-width">{{ site_slogan }}</div>
    {% endif %}
    </div>{# ./name-and-slogan #}
  {% endif %}
  <div class="nav-wrapper content page-width">
  <!-- Nav goes here -->
  </div>
</header>
<aside class="messages">
{{ messages }}
</aside>
<main role="main">
  <a id="main-content"></a>{# link is in html.html.twig #}
    {% if title %}
      <h1 class="page-title page-width">
        {{ title }}
      </h1>
    {% endif %}

    {% if content %}
    <div class="page-width">
      {{ content }}
    </div>
    {% endif %}


  <aside class="sidebar-first" role="complementary">
  </aside>

  <aside class="sidebar-second" role="complementary">
  </aside>

</main>

<footer role="contentinfo">
  {{ page.footer }}
</footer>

